   "source": [
    "n = 7\n",
    "twos = n//2\n",
    "ones = n%2\n",
    "res = []\n",
    "sublist = []\n",
    "while twos > 0 or ones <= n:\n",
    "    for i in range(twos):\n",
    "        sublist.append(2)\n",
    "    for i in range(ones):\n",
    "        sublist.append(1)\n",
    "    res.append(sublist)\n",
    "    sublist = []\n",
    "    twos -= 1\n",
    "    ones += 2\n",
    "print(res, len(res))"
   ]
  }
